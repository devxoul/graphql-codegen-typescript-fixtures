{
  "name": "graphql-codegen-typescript-fixtures",
  "version": "1.1.0",
  "description": "A plugin for graphql-code-generator that generates TypeScript fixtures for testing.",
  "main": "./build/index.js",
  "repository": "https://github.com/devxoul/graphql-codegen-typescript-fixtures.git",
  "author": "Suyeol Jeon <devxoul@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "clean": "rm -rf ./build ./tests/generated ./tests/schema",
    "build": "tsc",
    "typecheck": "yarn tsc -p tsconfig.json --noemit --skipLibCheck",
    "lint": "eslint . --ext .ts",
    "test": "jest"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.15.6",
    "@babel/preset-typescript": "^7.15.0",
    "@graphql-codegen/cli": "^2.6.2",
    "@graphql-codegen/typescript": "^2.4.5",
    "@types/dedent": "^0.7.0",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.31.2",
    "@typescript-eslint/parser": "^4.31.2",
    "babel-jest": "^27.2.0",
    "eslint": "^7.32.0",
    "graphql": "^16.3.0",
    "immer": "^9.0.6",
    "jest": "^27.2.0",
    "js-yaml": "^4.1.0",
    "typescript": "^4.4.3"
  },
  "dependencies": {
    "@graphql-codegen/schema-ast": "^2.2.0",
    "dedent": "^0.7.0"
  }
}
